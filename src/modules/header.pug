mixin navLink(link, name, last)
    li
        if(last)
            a(href=link).nav-link.last-link=name
        else
            a(href=link).nav-link=name

- var linkObj1 = {link: "#", name: "1"};
- var linkObj2 = {link: "#", name: "2"};
- var navLinks = [linkObj1,linkObj2]

header
    .header
        ul.nav
            for i in navLinks
                +navLink(i.link,i.name)
